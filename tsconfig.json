{
   "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "baseUrl": ".",
      "paths": {
         "@graphgen": ["graphgen/graphql"],
         "@components/*": ["components/*"],
         "@styles/*": ["styles/*"],
         "@apollo/*": ["apollo/*"],
         "@helpers/*": ["helpers/*"],
         "@lib": ["lib/firebase"],
         "@contexts": ["contexts/AuthContext"]
      },
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve"
   },
   "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "styles/ProfileStyles.ts"],
   "exclude": ["node_modules"]
}
